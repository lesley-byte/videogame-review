<section>
    <p class="card-title"> This is all-reviews.handlebars!</p>
    <ul>
        {{#each reviews as |review|}}
        <li class="card-title">Review title: {{review.review_title}}</li>
        <li>Review text: {{review.review_text}}</li>
        <li>Review date: {{review.review_date}}</li>
        <li>Review game name: {{review.game.game_name}}</li>
        <li>Review user name: {{review.user.username}}</li>
        {{/each}}
</section>